#include <stdio.h>
#include<stdlib.h>


//codigo pra liberar lista de tras pra frente.

typedef struct cel{
  int conteudo;
  struct cel *seg;
}cel;

typedef struct fila{
    int quantidade;
    int maximo;
    struct cel *inicio;
    struct cel *fim;
}Fila;


Fila* cria_fila(){
    Fila* fi= (Fila*)malloc(sizeof(Fila));
    if(fi!=NULL){
       fi->quantidade = 0;
       fi->maximo=6;
       fi->inicio=NULL;
       fi->fim=NULL;
    }
}

int enqueue(Fila* fi, int x){
  if(fi==NULL || fi->quantidade>= fi->maximo) {return -1;}
  cel* aux = (cel*) malloc(sizeof(cel));
  if(aux==NULL){return 0;}
  aux->conteudo = x;
  aux->seg = NULL;
  
  if(fi->quantidade==0){
      fi->inicio=aux;
      fi-> fim = aux;
  }
  else{
  fi->fim->prox= aux;
  fi-> fim=aux;
      
  }
  fi-> quantidade++;
}


int dequeue (Fila* fi){
 if(fi==NULL) {return 0;}
 
 *fi = (*fi)->fim;
 

}
int main(void) {
  Fila *lst;
  Fila *l=NULL;
  lst = cria_fila();
 enqueue(lst, 1);
 enqueue(lst, 2);
 enqueue(lst, 3);
 enqueue(lst, 4);
 enqueue(lst, 5);
  imprimirLista(lst);
  printf("\n");
  dequeue (lst);
 imprimirLista(lst);
 printf("\n");
 mostrarTopo(lst);
 isEmpty(lst);
 isEmpty(l);
 
  return 0;
}
